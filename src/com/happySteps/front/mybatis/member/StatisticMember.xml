<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.happySteps.front.mybatis.member.StatisticMember">
	
	<insert id="insert" parameterType="statisticMemberDto">
		INSERT INTO TB_STAT_MBR
			(SEQ_MBR, POST, DT_REG)
		VALUES
			(#{seq_mbr}, #{post}, SYSDATE)
	</insert>
	
</mapper>